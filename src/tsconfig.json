{
    "compilerOptions": {
        "target": "es5",
        "module": "es6",
        "lib": [
            "es6",
            "dom"
        ],
        "moduleResolution": "node",
        "baseUrl": "",
        "outDir": "../dist/out-tsc",
        "mapRoot": "./",
        "sourceMap": true,
        "typeRoots": [
            "../node_modules/@types"
        ],
        "newLine": "LF",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "alwaysStrict": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true
    }
}
